import org.apache.lucene.index.IndexReader;
import org.apache.lucene.search.Filter;

import java.io.IOException;
import java.util.BitSet;

/**
 * <p>
 * A ChainableFilter allows multiple filters to be chained,
 * the resulting bitset depending on the logic applied.
 * </p>
 * <p>
 * Order in which filters are called depends on
 * the position of the filter in the chain. It's probably
 * more efficient to place the most restrictive filters
 * /least computationally-intensive filters first.
 * </p>
 *
 * @author <a href=3D"mailto:kelvin@relevanz.com">Kelvin Tan</a>
  */
public class ChainableFilter extends Filter
{
    public static final int OR =3D 0;
    public static final int AND =3D 1;
    public static final int ANDNOT =3D 2;
    public static final int XOR =3D 3;

    private static final int DEFAULT =3D OR;

    /** The filter chain */
    private Filter[] chain =3D null;

    /**
     * Creates a new ChainableFilter.
     *
     * @param chain The chain of filters.
     */
    public ChainableFilter(Filter[] chain)
    {
        this.chain =3D chain;
    }

    public BitSet bits(IndexReader reader) throws IOException
    {
        return bits(reader, DEFAULT);
    }

    public BitSet bits(IndexReader reader, int logic) throws IOException
    {
        BitSet result =3D new BitSet(reader.maxDoc());
        for (int i =3D 0; i < chain.length; i++)
        {
            doChain(result, reader, logic, chain[i]);
        }
        return result;
    }

    public BitSet bits(IndexReader reader, int[] logic) throws =
IOException
    {
        BitSet result =3D new BitSet(reader.maxDoc());
        if (logic.length !=3D chain.length)
            throw new IllegalArgumentException("Invalid number of =
elements in logic array");
        for (int i =3D 0; i < chain.length; i++)
        {
            doChain(result, reader, logic[i], chain[i]);
        }
        return result;
    }

    private void doChain(BitSet result, IndexReader reader,
                         int logic, Filter filter) throws IOException
    {
        if(logic =3D=3D OR)
            result.or(filter.bits(reader));
        else if (logic =3D=3D AND)
            result.and(filter.bits(reader));
        else if (logic =3D=3D ANDNOT)
            result.andNot(filter.bits(reader));
        else if (logic =3D=3D XOR)
            result.xor(filter.bits(reader));
        else
            doChain(result, reader, DEFAULT, filter);
    }
}

